<div class="modal fade" id="{{ modalId }}" tabindex="-1" aria-labelledby="{{ modalId }}Label" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content bg-dark">
			{% if modalTitle is defined %}
				<div class="modal-header">
				<h5 class="modal-title fs-5" id="{{ modalId }}Label">{{ modalTitle }}</h5>
				<button type="button" class="btn btn-dark" data-bs-dismiss="modal" aria-label="Close">
					<i class="fas fa-close"></i>
				</button>
			</div>
			{% endif %}
			{% if modalBody is defined %}
				<div class="modal-body">
				{{ modalBody }}
			</div>
			{% endif %}
			{% if modalFooter is defined %}
				<div class="modal-footer">
				{{ modalFooter }}
			</div>
			{% endif %}
		</div>
	</div>
</div>
